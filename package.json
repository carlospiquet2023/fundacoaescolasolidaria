{
  "name": "fundacao-escola-solidaria",
  "version": "2.0.0",
  "description": "Sistema institucional profissional da Fundação Escola Solidária com CMS e API REST",
  "main": "server/server.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "client:dev": "vite",
    "server:dev": "nodemon server/server.js",
    "start": "node server/server.js",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src server --ext .js --fix",
    "format": "prettier --write \"**/*.{js,css,html}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "analyze": "vite-bundle-visualizer",
    "seed": "node server/seed.js",
    "init-db": "node server/init-database.js"
  },
  "keywords": [
    "educação",
    "inclusão",
    "solidariedade",
    "fundação",
    "escola"
  ],
  "author": "Fundação Escola Solidária",
  "license": "MIT",
  "devDependencies": {
    "@vitejs/plugin-legacy": "^5.3.0",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "nodemon": "^3.0.3",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "tailwindcss": "^3.4.1",
    "vite": "^5.0.12",
    "vite-bundle-visualizer": "^1.0.1",
    "vitest": "^1.2.1"
  },
  "dependencies": {
    "axios": "^1.6.5",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.19.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
